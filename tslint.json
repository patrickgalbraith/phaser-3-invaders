{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "ordered-imports": false,
    "no-require-imports": false,
    "trailing-comma": false,
    "semicolon": false,
    "space-before-function-paren": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true, "single", "avoid-escape"
    ]
  }
}